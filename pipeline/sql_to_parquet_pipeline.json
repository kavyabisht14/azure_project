{
	"name": "sql_to_parquet_pipeline",
	"properties": {
		"activities": [
			{
				"name": "Copy_sql_to_adls_parquet",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Date",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "Date",
									"type": "String",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "Close",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "Close",
									"type": "String",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "High",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "High",
									"type": "String",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "Low",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "Low",
									"type": "String",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "Open",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "Open",
									"type": "String",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "Volume",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "Volume",
									"type": "String",
									"physicalType": "UTF8"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "source_adls_to_parquet",
						"type": "DatasetReference",
						"parameters": {
							"tableName": {
								"value": "@pipeline().parameters.TableName",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "adls_parquet_bronze",
						"type": "DatasetReference",
						"parameters": {
							"tableName": {
								"value": "@pipeline().parameters.TableName",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"TableName": {
				"type": "string",
				"defaultValue": "MSFT"
			}
		},
		"annotations": []
	}
}